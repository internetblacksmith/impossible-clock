{
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "snyk": "1.1291.1",
    "start-server-and-test": "2.0.12"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve:test": "vite --mode test",
    "cy:run": "cypress run",
    "test": "start-server-and-test 'npm run serve:test' http://localhost:3000 'npm run cy:run'",
    "cy:open": "cypress open",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  },
  "snyk": true,
  "devDependencies": {
    "autoprefixer": "10.4.21",
    "cssnano": "7.1.0",
    "cypress": "14.5.3",
    "cypress-image-diff-js": "^2.4.0",
    "netlify-plugin-cypress": "2.2.1",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "sass": "1.89.2",
    "vite": "7.0.6"
  }
}
