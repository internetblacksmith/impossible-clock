{
  "all": true,
  "include": [
    "app/scripts/**/*.js"
  ],
  "exclude": [
    "**/*.spec.js",
    "**/*.test.js",
    "node_modules/**",
    "dist/**",
    "cypress/**",
    "vite.config.js"
  ],
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}